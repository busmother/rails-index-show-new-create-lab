<h3>Coupon Form</h3>
<%=form_tag coupons_path do%>

    <label>Coupon Code:</label><br>
    <%=text_field_tag id: 'coupon_code', name: 'coupon[coupon_code]'%><br>

    <label>Store:</label><br>
    <%=text_field_tag id: 'store', name: 'coupon[store]'%>

    <%= submit_tag "Submit Coupon"%>
<%end%>

<%=params.inspect%>